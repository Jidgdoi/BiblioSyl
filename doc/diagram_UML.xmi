    <sourcecode value="def setAttr(self, attr, value):&#xa;&#x9;&quot;&quot;&quot;&#xa;&#x9;Set the attribute with the given value.&#xa;&#x9;Params are: 'auteurPrenom', 'nbPage', 'commentaire', 'couverture', 'lieu', 'parution', 'note', 'auteurNom', 'editeur', 'genre', 'titre'&#xa;&#x9;&quot;&quot;&quot;&#xa;&#x9;if attr in [&quot;parution&quot;, &quot;nbPage&quot;, &quot;note&quot;]:&#xa;&#x9;&#x9;try:&#xa;&#x9;&#x9;&#x9;self.__dict__[attr] = int(value)&#xa;&#x9;&#x9;except ValueError:&#xa;&#x9;&#x9;&#x9;print &quot;Error: your value '%s' is inapropriate for the attribute '%s'. You have to specify an integer.&quot; %(value, attr)&#xa;&#x9;elif attr in self.__dict__:&#xa;&#x9;&#x9;self.__dict__[attr] = value&#xa;&#x9;else:&#xa;&#x9;&#x9;print &quot;Error: this attribute doesn't exist: %s.&quot; %attr" id="Sx3F5WKtsBHD"/>
    <sourcecode value="def updateID(self):&#xa;&#x9;&quot;&quot;&quot;&#xa;&#x9;Créé un identifiant 'ID' unique pour le livre&#xa;&#x9;ID: Initiales de l'auteur + somme des valeurs ASCII du titre et de l'éditeur&#xa;&#x9;&quot;&quot;&quot;&#xa;&#x9;ID = self.auteurPrenom[:1] or &quot;Z&quot;&#xa;&#x9;ID += self.auteurNom[:1] or &quot;Z&quot;&#xa;&#x9;ID += str(sum([ord(i) for i in self.titre+self.editeur]))&#xa;&#x9;self.ID = ID" id="hXiS1moufLPz"/>
    <sourcecode value="def uploadBook(self, pathFile):&#xa;&#x9;&quot;&quot;&quot;&#xa;&#x9;Return an object Livre.&#xa;&#x9;'pathFile': path to file.lsc&#xa;&#x9;&quot;&quot;&quot;&#xa;&#x9;attributes = []&#xa;&#x9;with open(pathFile,'r') as f:&#xa;&#x9;&#x9;for line in f:&#xa;&#x9;&#x9;&#x9;attributes.append(line.strip())&#xa;&#x9;attributes[9] = attributes[9].replace(self.rplc, '\n')&#x9;# On remplace les '\N' par de vrais retour chariot&#xa;&#x9;# TEMPORARY &#xa;&#x9;if len(attributes) != 13:&#xa;&#x9;&#x9;attributes.extend([&quot;&quot;, []])&#xa;&#x9;# Get the several readers from the attr &quot;read&quot;, separated by comma&#xa;&#x9;if attributes[12]:&#xa;&#x9;&#x9;attributes[12] = attributes[12].split(',')&#xa;&#x9;# Create book&#xa;&#x9;newBook = Livre(attributes[0],&#x9;&#x9;# auteurPrenom&#xa;&#x9;&#x9;&#x9;&#x9;&#x9;attributes[1],&#x9;&#x9;# auteurNom&#xa;&#x9;&#x9;&#x9;&#x9;&#x9;attributes[2],&#x9;&#x9;# titre&#xa;&#x9;&#x9;&#x9;&#x9;&#x9;attributes[3],&#x9;&#x9;# editeur&#xa;&#x9;&#x9;&#x9;&#x9;&#x9;attributes[4],&#x9;&#x9;# parution&#xa;&#x9;&#x9;&#x9;&#x9;&#x9;attributes[5],&#x9;&#x9;# genre&#xa;&#x9;&#x9;&#x9;&#x9;&#x9;attributes[6],&#x9;&#x9;# nbPage&#xa;&#x9;&#x9;&#x9;&#x9;&#x9;attributes[7],&#x9;&#x9;# lieu&#xa;&#x9;&#x9;&#x9;&#x9;&#x9;attributes[8],&#x9;&#x9;# note&#xa;&#x9;&#x9;&#x9;&#x9;&#x9;attributes[9],&#x9;&#x9;# commentaire&#xa;&#x9;&#x9;&#x9;&#x9;&#x9;attributes[10],&#x9;&#x9;# couverture&#xa;&#x9;&#x9;&#x9;&#x9;&#x9;attributes[11],&#x9;&#x9;# ISBN&#xa;&#x9;&#x9;&#x9;&#x9;&#x9;attributes[12])&#x9;&#x9;# read&#xa;&#x9;return newBook" id="Tm5mlXfggWWZ"/>
    <sourcecode value="def uploadLibrary(self, path):&#xa;&#x9;&quot;&quot;&quot;&#xa;&#x9;Charge les livres sauvegarder dans le dossier 'path'.&#xa;&#x9;'path': path to the books files.&#xa;&#x9;&quot;&quot;&quot;&#xa;&#x9;# --- Liste des fichiers contenus dans le dossier pathLivre&#xa;&#x9;lLivre = os.listdir(path)&#xa;&#x9;lLivre = [ x for x in lLivre if x[-4:] == self.extLivre]&#xa;&#x9;# --- On ouvre chaque fichier&#xa;&#x9;for i in lLivre:&#xa;&#x9;&#x9;newBook = self.uploadBook(path+i)&#xa;&#x9;&#x9;ID = i.split(self.OS)[-1].split(&quot;.&quot;)[0]&#xa;&#x9;&#x9;# On ajoute le livre au dico biblio avec pour clé son ID (nom du fichier)&#xa;&#x9;&#x9;self.biblio[ID] = newBook&#xa;&#x9;&#x9;self.win32Decoding(ID)" id="rXidt40Kq37Z"/>
    <sourcecode value="def addBook(self, book, forcer=False):&#xa;&#x9;&quot;&quot;&quot;&#xa;&#x9;Add the new book to the library.&#xa;&#x9;L'attribut 'forcer' signifie que l'on force l'ajout du livre, bien que celui-ci semble déjà exister.&#xa;&#x9;&quot;&quot;&quot;&#xa;&#x9;ID = book.getAttr('ID')&#xa;&#x9;# --- On force l'ajout du livre: création d'une ID similaire&#xa;&#x9;if forcer:&#xa;&#x9;&#x9;alphabet = string.ascii_letters&#xa;&#x9;&#x9;ite = 0&#xa;&#x9;&#x9;ID += alphabet[ite]&#xa;&#x9;&#x9;while ID in self.biblio.keys():&#xa;&#x9;&#x9;&#x9;ite += 1&#xa;&#x9;&#x9;&#x9;if ite == len(alphabet):&#xa;&#x9;&#x9;&#x9;&#x9;ite = 0&#xa;&#x9;&#x9;&#x9;&#x9;ID += alphabet[ite]&#xa;&#x9;&#x9;&#x9;else:&#xa;&#x9;&#x9;&#x9;&#x9;ID = ID[:-1] + alphabet[ite]&#xa;&#x9;# On vérifie si l'ID n'existe pas déjà&#xa;&#x9;else:&#xa;&#x9;&#x9;# --- Check si le livre existe déjà&#xa;&#x9;&#x9;if ID in self.biblio.keys():&#xa;&#x9;&#x9;&#x9;return True, ID&#xa;&#x9;&#xa;&#x9;# --- On l'ajoute au dico biblio et on le sauvegarde dans un fichier '.lsc' avec pour nom son ID.&#xa;&#x9;self.biblio[ID] = livre&#xa;&#x9;self.saveBook(ID)&#xa;&#x9;self.win32Decoding(ID)&#xa;&#x9;return False, ID" id="VSi4mVCe1uFu"/>
    <sourcecode value="def deleteBook(self, ID):&#xa;&#x9;&quot;&quot;&quot;&#xa;&#x9;Remove the Id and his book from the library, and delete the file.&#xa;&#x9;&quot;&quot;&quot;&#xa;&#x9;# --- On supprime le fichier correspondant à ce livre&#xa;&#x9;os.remove(self.pathLivre + ID + self.extLivre)&#xa;&#x9;&#xa;&#x9;# --- On supprime l'image de couverture si celle-ci n'est pas l'image par défaut, et si la couverture n'est pas utilisé par un autre livre&#xa;&#x9;if self.biblio[ID].couverture != 'image-not-available.jpg':&#xa;&#x9;&#x9;if len([k for k in self.biblio.keys() if self.biblio[k].couverture == self.biblio[ID].couverture]) &lt;= 1:&#xa;&#x9;&#x9;&#x9;os.remove(self.pathCouv + self.biblio[ID].couverture)&#xa;&#x9;&#xa;&#x9;# --- On supprime le livre des biblios&#xa;&#x9;self.biblio.pop(ID)&#xa;&#x9;return 0" id="AkFADG7hhnlT"/>
    <sourcecode value="def modifyBook(self, ID, modif):&#xa;&#x9;&quot;&quot;&quot;&#xa;&#x9;Modifie un livre de la liste.&#xa;&#x9;&quot;&quot;&quot;&#xa;&#x9;# --- Actualisation du livre&#xa;&#x9;for i in modif.keys():&#xa;&#x9;&#x9;self.biblio[ID].setAttr(i,modif[i])&#xa;&#x9;# --- MAJ de l'ID du livre&#xa;&#x9;self.biblio[ID].updateID()&#xa;&#x9;# --- Si la nouvelle ID est différente de la précédente, on modifie la référence dans le dico&#xa;&#x9;newID = self.biblio[ID].getAttr('ID')&#xa;&#x9;if newID != ID:&#xa;&#x9;&#x9;self.biblio[newID] = self.biblio.pop(ID)&#xa;&#x9;&#xa;&#x9;# --- Remplacement du fichier livre (suppression de la vesion obsolète du livre, et création d'un nouveau fichier)&#xa;&#x9;os.remove(self.pathLivre + ID + self.extLivre)&#xa;&#x9;self.saveBook(newID)&#xa;&#x9;&#xa;&#x9;# --- Decode le nouveau livre pour win32&#xa;&#x9;self.win32Decoding(newID)&#xa;&#x9;return 0" id="imToFQBPAQZN"/>
